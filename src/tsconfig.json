{
  "compilerOptions": {
    "outDir": "../dist",
    "module": "commonjs",
    "target": "es6",
    "declaration" :true,
    "inlineSourceMap": true,
    "moduleResolution": "node",
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "stripInternal": true,
    "noEmitOnError": false,
    "removeComments": false,
    "lib": ["dom", "es6"],
    "typeRoots": ["../../node_modules/@types"],
    "types": ["node", "webpack"],
    "baseUrl": ""
  }
}
